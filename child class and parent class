#include <stdio.h>
#include <unistd.h>

int main() {
    pid_t child_pid, parent_pid;

    if (child_pid < 0) {
        
        perror("Fork failed");
        return 1;
    }

    if (child_pid == 0) {

        printf("Child Process:\n");
        printf("Child PID: %d\n", getpid());
        
    } else {
        
        printf("Parent Process:\n");
        printf("Parent PID: %d\n", getpid());
        printf("Child PID: %d\n", child_pid);
    }

    return 0;
}
